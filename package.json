{
  "name": "edex-chatbot",
  "version": "1.0.0",
  "description": "eDEX-UI style chatbot application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder",
    "postinstall": "electron-builder install-app-deps",
    "rebuild": "electron-rebuild",
    "install-deps": "npm install && npm run rebuild"
  },
  "devDependencies": {
    "electron": "^21.0.0",
    "electron-builder": "^24.13.3",
    "electron-rebuild": "^3.2.9"
  },
  "keywords": [
    "edex",
    "chatbot",
    "electron",
    "ui"
  ],
  "author": "eDEX Chatbot Team",
  "license": "MIT",
  "build": {
    "appId": "com.edex.chatbot",
    "productName": "eDEX Chatbot",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "main.js",
      "src/preload.js"
    ],
    "win": {
      "target": "nsis"
    },
    "mac": {
      "category": "public.app-category.utilities"
    },
    "linux": {
      "target": "AppImage"
    }
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.57.2",
    "axios": "^1.11.0",
    "body-parser": "^2.2.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "electron-squirrel-startup": "^1.0.1",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "prismjs": "^1.30.0",
    "rate-limiter-flexible": "^7.3.0",
    "sequelize": "^6.37.7",
    "sqlite3": "^5.1.7",
    "styled-components": "^6.1.19",
    "uuid": "^8.3.2"
  }
}
